<library>
    <compatible>
        <keycode>size</keycode>
        <keydesc>分辨率</keydesc>
        <defaultvalue>1</defaultvalue>
        <inputtype>select</inputtype>
        <optionjson>[{
            "width": "1920",
            "height": "1080"
            },
            {
            "width": "800",
            "height": "600"
            }, {
            "width": "400",
            "height": "320"
            }
            ]</optionjson>
    </compatible>

    <compatible>
        <keycode>isMax</keycode>
        <keydesc>是否最大化</keydesc>
        <defaultvalue>0</defaultvalue>
        <inputtype>select</inputtype>
        <optionjson>[{
            "value": "true",
            {
            "value": "false",
            }
            ]</optionjson>
    </compatible>


    <compatible>
        <keycode>maxWidth</keycode>
        <keydesc>最大宽度</keydesc>
        <defaultvalue>1920</defaultvalue>
        <inputtype>input</inputtype>
        <optionjson>[]</optionjson>
    </compatible>

</library>